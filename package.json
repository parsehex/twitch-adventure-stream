{
  "name": "vite-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build-css && vite build",
    "dev": "concurrently \"npm run watch-ts\" \"npm:wait\" \"vite --host 0.0.0.0\"",
    "build-css": "npx tailwindcss build -i src/index.css -o public/styles.css",
    "watch-server": "nodemon server-out/main.js",
    "watch-ts": "tsc --watch -p server/tsconfig.json",
    "watch-css": "npx tailwindcss build -w -i src/index.css -o public/styles.css",
    "wait": "wait-on server-out/main.js && concurrently \"npm:watch-server\" \"npm:watch-css\"",
    "llamacpp:win": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command .\\start_llamacpp.ps1"
  },
  "dependencies": {
    "@orama/orama": "^2.0.19",
    "@vueuse/core": "^10.9.0",
    "ai": "^3.0.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "embla-carousel": "^8.0.4",
    "embla-carousel-vue": "^8.0.4",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "lucide-vue-next": "^0.378.0",
    "openai": "^4.47.3",
    "pinia": "^2.1.7",
    "radix-vue": "^1.7.3",
    "speak-tts": "^2.0.8",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tiktoken": "^1.0.15",
    "vite-tsconfig-paths": "^4.3.2",
    "vue": "^3.4.23",
    "wait-on": "^7.2.0",
    "xstate": "^5.13.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.1.5"
  }
}
